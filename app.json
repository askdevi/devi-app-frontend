{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/devi-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hrish17.boltexponativewind",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select and share images in your chat."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/devi-icon.png"
    },
    "splash": {
      "image": "./assets/images/devi-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#342661"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.hrish17.boltexponativewind"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a9e840f6-017e-440f-872e-b2afa3f42e3e"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a9e840f6-017e-440f-872e-b2afa3f42e3e"
    }
  }
}